<?xml version='1.1' encoding='UTF-8'?>
<slave>
  <name>SLURM.RM.001</name>
  <description>Agent dedicated for Release Management Team Runs</description>
  <remoteFS>/scratch/jenkins</remoteFS>
  <numExecutors>1</numExecutors>
  <mode>EXCLUSIVE</mode>
  <retentionStrategy class="hudson.slaves.RetentionStrategy$Demand">
    <inDemandDelay>0</inDemandDelay>
    <idleDelay>1</idleDelay>
  </retentionStrategy>
  <launcher class="org.jenkinsci.plugins.slave_setup.SetupSlaveLauncher" plugin="slave-setup@1.10">
    <launcher class="hudson.slaves.JNLPLauncher">
      <workDirSettings>
        <disabled>false</disabled>
        <internalDir>remoting</internalDir>
        <failIfWorkDirIsMissing>false</failIfWorkDirIsMissing>
      </workDirSettings>
      <webSocket>false</webSocket>
    </launcher>
    <startScript>(cd ~/jenkins-utils &amp;&amp; git pull git@github.com:sifive/jenkins-utils.git) &amp;&amp; \
AGENT_NAME=SLURM.RM.001 \
JENKINS_AGENT_SLURM_PARTITION=redhat_cloud \
JNLP_SECRET=ad9b0f0647127024805e754e9a46d9c8d8be807b0ad6c822e6774378772871eb \
JENKINS_URL=https://jenkins.internal.sifive.com/ \
~/jenkins-utils/tools/jenkins-agent-salloc-release-management.sh</startScript>
    <stopScript></stopScript>
  </launcher>
  <label>slurm-rm slurm-rm-standard slurm-rm-standard-s</label>
  <nodeProperties/>
</slave>