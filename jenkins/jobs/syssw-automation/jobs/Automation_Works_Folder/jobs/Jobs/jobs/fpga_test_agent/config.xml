export FUSDK_MACHINE="${FUSDK_MACHINE}"
export META_SIFIVE_BRANCH="${META_SIFIVE_BRANCH}"
export FUSDK_MANIFEST="${FUSDK_MANIFEST}"
export NFS_Path="${NFS_Path}"
