export tests_to_run="${tests_to_run}"
export syssw_automation_tools_branch_name="${syssw_automation_tools_branch_name}"
export qemu_memory="${qemu_memory}"
export Test_Result_Save_Path="${Test_Result_Save_Path}"
export public_or_private="${public_or_private}"
export Dispatcher_Id="${Dispatcher_Id}"


# for debugging
echo $NODE_NAME
hostname
build_date=`date +%m-%d-%Y`
echo "[INFO] Jenkins build"
echo "[DEBUG] build date = &lt;$build_date&gt;"
echo "[DEBUG] USER=$USER"
echo "[DEBUG] HOME=$HOME"
echo "[DEBUG] PWD=$PWD"
echo "[DEBUG] WORKSPACE=$WORKSPACE/$BUILD_ID"
WORKSPACE="$WORKSPACE/$BUILD_ID"
#echo "git config --global --list"

#git config --global --replace-all url."git@github.com:".insteadOf ssh://git@github.com/
#git config --global --add url."git@github.com:".insteadOf git://git@github.com/

#git config --global --list

lsb_release -a

##
set -x
mkdir -p ${WORKSPACE}
cd ${WORKSPACE}
git clone -b ${syssw_automation_tools_branch_name} --recursive git@github.com:sifive/syssw_automation_tools.git
chmod 755 ./syssw_automation_tools/jenkins_scripts/jobs/fpga_test_agent.sh
./syssw_automation_tools/jenkins_scripts/jobs/fpga_test_agent.sh

set +x
##</command>
