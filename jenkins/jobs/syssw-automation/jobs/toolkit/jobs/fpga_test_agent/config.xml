        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>GLOBAL_CONFIG</name>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>TEST_CONFIG</name>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.TextParameterDefinition>
          <name>HW_CONFIG</name>
          <defaultValue>{ }</defaultValue>
          <trim>false</trim>
        </hudson.model.TextParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>Dispatcher_Id</name>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>NFS_Path</name>
          <defaultValue>/work/jenkins/syssw</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>NFS_WORKSPACE</name>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>HWCONFIG_NAME</name>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>TOP_BUILD_NAME</name>
          <defaultValue>(no upstream)</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
      </parameterDefinitions>
    </hudson.model.ParametersDefinitionProperty>
    <hudson.plugins.throttleconcurrents.ThrottleJobProperty plugin="throttle-concurrents@2.6">
      <maxConcurrentPerNode>0</maxConcurrentPerNode>
      <maxConcurrentTotal>0</maxConcurrentTotal>
      <categories class="java.util.concurrent.CopyOnWriteArrayList"/>
      <throttleEnabled>false</throttleEnabled>
      <throttleOption>project</throttleOption>
      <limitOneJobWithMatchingParams>false</limitOneJobWithMatchingParams>
      <paramsToUseForLimit/>
    </hudson.plugins.throttleconcurrents.ThrottleJobProperty>
    
  </properties>
  <scm class="hudson.scm.NullSCM"/>
  <assignedNode>sw-vcu118</assignedNode>
  <canRoam>false</canRoam>
  <disabled>false</disabled>
  <blockBuildWhenDownstreamBuilding>false</blockBuildWhenDownstreamBuilding>
  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
  <triggers/>
  <concurrentBuild>true</concurrentBuild>
  <builders>
    <hudson.tasks.Shell>
      <command>#!/bin/bash

set -x
export commit_hash=`readlink -f ${NFS_Path}/${Build_Sub_Path}-${public_or_private}/${FUSDK_MACHINE}/${META_SIFIVE_BRANCH} | xargs basename`</command>
      <configuredLocalRules/>
    </hudson.tasks.Shell>
    <hudson.tasks.Shell>
      <command>#!/bin/bash

echo "[INFO] hostname: $(hostname)"
if [ "${DRY_RUN}" == "true" ]; then
    exit 0
fi

lsb_release -a

##
set -x
cd ${WORKSPACE}

syssw_automation_tools_branch_name=$(echo "${GLOBAL_CONFIG}" | jq -r ".syssw_automation_tools_branch_name")

if [ ! -d "syssw_automation_tools" ]; then
    git clone -b ${syssw_automation_tools_branch_name} --recursive git@github.com:sifive/syssw_automation_tools.git syssw_automation_tools
else
    (
        cd syssw_automation_tools
        git fetch --all -p
        git reset --hard origin/${syssw_automation_tools_branch_name}
        git submodule init
        git submodule update --recursive
    )
fi

chmod 755 ./syssw_automation_tools/jenkins_scripts/jobs/fpga_test_agent.sh
./syssw_automation_tools/jenkins_scripts/jobs/fpga_test_agent.sh

set +x
##</command>
      <configuredLocalRules/>
    </hudson.tasks.Shell>
  </builders>
