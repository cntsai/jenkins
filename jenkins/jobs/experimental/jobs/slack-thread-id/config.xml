  <definition class="org.jenkinsci.plugins.workflow.cps.CpsFlowDefinition" plugin="workflow-cps@2.90">
    <script>pipeline {
    agent { label &apos;tiny&apos; }

    stages {
        stage(&apos;Send Slack&apos;) {
            steps {
                script {
                    def slackResponse = slackSend(channel: &quot;gcc-test-ci&quot;, message: &quot;Here is the primary message&quot;)
                    println slackResponse.threadId
                }
            }
        }
    }
}
</script>
    <sandbox>true</sandbox>
