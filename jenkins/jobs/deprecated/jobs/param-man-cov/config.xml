<?xml version='1.1' encoding='UTF-8'?>
<flow-definition plugin="workflow-job@2.39">
  <actions/>
  <description>Manually run a portion of federation-master-manual-trigger or nightly/federation-clean-nightly-trigger&#xd;
&lt;BR&gt;&#xd;
&#xd;
&lt;BR&gt;&#xd;
&lt;B&gt;NOTE:&lt;/B&gt; if you run test-nightly, your coverage data will be included in &lt;a href=https://coverage.internal.sifive.com/&gt;coverage.internal.sifive.com&lt;/a&gt;&#xd;
</description>
  <displayName>parameterized-manual-coverage</displayName>
  <keepDependencies>false</keepDependencies>
  <properties>
    <hudson.plugins.jira.JiraProjectProperty plugin="jira@3.1.1"/>
    <jenkins.model.BuildDiscarderProperty>
      <strategy class="hudson.tasks.LogRotator">
        <daysToKeep>14</daysToKeep>
        <numToKeep>-1</numToKeep>
        <artifactDaysToKeep>-1</artifactDaysToKeep>
        <artifactNumToKeep>-1</artifactNumToKeep>
      </strategy>
    </jenkins.model.BuildDiscarderProperty>
    <org.jenkinsci.plugins.workflow.job.properties.DisableResumeJobProperty/>
    <hudson.model.ParametersDefinitionProperty>
      <parameterDefinitions>
        <hudson.model.StringParameterDefinition>
          <name>CONFIG_SET</name>
          <description>&lt;i&gt;config-sets&lt;/i&gt; are defined &lt;a href=https://github.com/sifive/federation/blob/master/test_plan.yml#L827&gt;here&lt;/a&gt;

&lt;BR&gt;
&lt;BR&gt;
Also, &lt;i&gt;pseudo-&lt;/i&gt;config-sets for FPGAs:
&lt;ul&gt;
  &lt;li&gt;jenkins_fpga_arty&lt;/li&gt;
  &lt;li&gt;jenkins_fpga_arty_100t&lt;/li&gt;
  &lt;li&gt;jenkins_fpga_vc707&lt;/li&gt;
&lt;ul&gt;</description>
          <defaultValue>coreip_caboose</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.ChoiceParameterDefinition>
          <name>SUITE</name>
          <description>&lt;B&gt;NOTE:&lt;/B&gt; &lt;i&gt;suite&lt;/i&gt; also includes shorter suites (e.g. &lt;i&gt;test-long&lt;/i&gt; will also run &lt;i&gt;test-short&lt;/i&gt;)
&lt;BR&gt;
&lt;BR&gt;

Mapping of suite to Jenkins jobs:&lt;BR&gt;
&lt;ul&gt;
  &lt;li&gt;&lt;B&gt;federation-pull-request&lt;/B&gt;:
    &lt;ul&gt;&lt;li&gt;test-short&lt;/li&gt;&lt;/ul&gt;
  &lt;/li&gt;
  &lt;li&gt;&lt;B&gt;federation-master&lt;/B&gt;:
    &lt;ul&gt;&lt;li&gt;test-short&lt;/li&gt;&lt;/ul&gt;
    &lt;ul&gt;&lt;li&gt;test-long&lt;/li&gt;&lt;/ul&gt;
  &lt;/li&gt;
  &lt;li&gt;&lt;B&gt;federation-test-long-manual-trigger&lt;/B&gt;:
    &lt;ul&gt;&lt;li&gt;test-short&lt;/li&gt;&lt;/ul&gt;
    &lt;ul&gt;&lt;li&gt;test-long&lt;/li&gt;&lt;/ul&gt;
  &lt;/li&gt;
  &lt;li&gt;&lt;B&gt;torture/master&lt;/B&gt;:
    &lt;ul&gt;&lt;li&gt;test-torture&lt;/li&gt;&lt;/ul&gt;
  &lt;/li&gt;
  &lt;li&gt;&lt;B&gt;nightly/federation-clean-nightly-trigger&lt;/B&gt;:
    &lt;ul&gt;&lt;li&gt;test-short&lt;/li&gt;&lt;/ul&gt;
    &lt;ul&gt;&lt;li&gt;test-long&lt;/li&gt;&lt;/ul&gt;
    &lt;ul&gt;&lt;li&gt;test-nightly&lt;/li&gt;&lt;/ul&gt;
    &lt;ul&gt;&lt;li&gt;test-torture&lt;/li&gt;&lt;/ul&gt;
  &lt;/li&gt;
&lt;/ul&gt;</description>
          <choices class="java.util.Arrays$ArrayList">
            <a class="string-array">
              <string>test-short</string>
              <string>test-torture</string>
              <string>test-long</string>
              <string>test-nightly</string>
            </a>
          </choices>
        </hudson.model.ChoiceParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>GIT_COMMIT</name>
          <description>This can be your branch name or a hash pushed to github</description>
          <defaultValue>master</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>RUNSUITE_NUM_THREADS</name>
          <description>Select the number of threads for runsuite</description>
          <defaultValue>12</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.BooleanParameterDefinition>
          <name>SITEST_DELETE_ON_PASS</name>
          <description>If selected then passing simulation directory will be removed once simulation finishes.</description>
          <defaultValue>false</defaultValue>
        </hudson.model.BooleanParameterDefinition>
      </parameterDefinitions>
    </hudson.model.ParametersDefinitionProperty>
    <hudson.plugins.throttleconcurrents.ThrottleJobProperty plugin="throttle-concurrents@2.0.2">
      <maxConcurrentPerNode>0</maxConcurrentPerNode>
      <maxConcurrentTotal>0</maxConcurrentTotal>
      <categories class="java.util.concurrent.CopyOnWriteArrayList"/>
      <throttleEnabled>false</throttleEnabled>
      <throttleOption>project</throttleOption>
      <limitOneJobWithMatchingParams>false</limitOneJobWithMatchingParams>
      <paramsToUseForLimit></paramsToUseForLimit>
    </hudson.plugins.throttleconcurrents.ThrottleJobProperty>
    <org.jenkinsci.plugins.workflow.job.properties.PipelineTriggersJobProperty>
      <triggers/>
    </org.jenkinsci.plugins.workflow.job.properties.PipelineTriggersJobProperty>
  </properties>
  <definition class="org.jenkinsci.plugins.workflow.cps.CpsScmFlowDefinition" plugin="workflow-cps@2.81">
    <scm class="hudson.plugins.git.GitSCM" plugin="git@4.3.0">
      <configVersion>2</configVersion>
      <userRemoteConfigs>
        <hudson.plugins.git.UserRemoteConfig>
          <url>git@github.com:sifive/federation.git</url>
          <credentialsId>ad25d95a-4a30-4cc9-bda9-e836d51af0b9</credentialsId>
        </hudson.plugins.git.UserRemoteConfig>
      </userRemoteConfigs>
      <branches>
        <hudson.plugins.git.BranchSpec>
          <name>${GIT_COMMIT}</name>
        </hudson.plugins.git.BranchSpec>
      </branches>
      <doGenerateSubmoduleConfigurations>false</doGenerateSubmoduleConfigurations>
      <submoduleCfg class="list"/>
      <extensions/>
    </scm>
    <scriptPath>jenkins/nightly.Jenkinsfile</scriptPath>
    <lightweight>false</lightweight>
  </definition>
  <triggers/>
  <disabled>false</disabled>
</flow-definition>