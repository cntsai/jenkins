  <builders>
    <hudson.tasks.Shell>
      <command>#!/bin/bash -x

echo "Install dependencies"
python3 -m pip install --user --upgrade pip
python3 -m pip install $(find toolchain-bumper -name "requirements.txt" | sed -En "s/^/-r /p")

echo "Sync"
META_REPO_SSH="git@github.com:sifive/$META_REPO.git"
git clone $META_REPO_SSH

$WORKSPACE/toolchain-bumper/$BUMPER --config-module=$META_CONF_REPO --meta-repo=$META_REPO sync --dump-submodule-info || exit 1
rm -rf $META_REPO</command>
      <configuredLocalRules/>
    </hudson.tasks.Shell>
